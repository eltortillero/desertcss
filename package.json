{
  "name": "desertcss",
  "version": "1.0.0",
  "description": "scss and css library",
  "main": "index.js",
  "author": "eltortillero",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf package && webpack --config webpack.config.dev.js --mode development",
    "dev:watch": "rm -rf package && webpack --config webpack.config.dev.js --mode development --watch",
    "build": "rm -rf package && webpack --config webpack.config.prod.js --mode production",
    "build:production": "rm -rf package && webpack --config webpack.config.dev.js --mode development && webpack --config webpack.config.prod.js --mode production"
  },
  "dependencies": {
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "file-loader": "^6.2.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "webpack-cli": "^4.10.0"
  }
}